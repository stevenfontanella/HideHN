{
  "manifest_version": 2,
    "name": "Hide HN",
    "version": "0.2",

    "description": "Hide negative posts from Hacker News",

    "content_scripts": [
      {
          "matches": ["*://*.news.ycombinator.com/",
                      "*://*.news.ycombinator.com/news*"],
        "js": ["main.js"]
      }
    ],

    "permissions": [
        "https://eutpj14kkj.execute-api.us-west-2.amazonaws.com/prod"
    ]
}
